; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Craftable",
"language": "GDScript",
"path": "res://scripts/crafting/Craftable.gd"
}, {
"base": "Control",
"class": "MissionBasic",
"language": "GDScript",
"path": "res://scripts/MissionBasic.gd"
}, {
"base": "Node",
"class": "NodeAnimated",
"language": "GDScript",
"path": "res://scripts/animate/NodeAnimated.gd"
} ]
_global_script_class_icons={
"Craftable": "",
"MissionBasic": "",
"NodeAnimated": ""
}

[application]

config/name="Surv"
run/main_scene="res://nodes/Game.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://scripts/Global.gd"
Inventory="*res://scripts/Inventory.gd"
Player="*res://scripts/Player.gd"
Buildings="*res://scripts/Buildings.gd"
Tools="*res://scripts/tools.gd"
Events="*res://scripts/Events.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="2d"
window/stretch/aspect="expand"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
